-- Main Cheat Script with UI and Cheats (Flight, 1x1x1x1, c00lkidd's World Manipulation, Speed Hack)

-- Services
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
local userInputService = game:GetService("UserInputService")
local runService = game:GetService("RunService")
local gui = Instance.new("ScreenGui", game.CoreGui)
gui.Name = "ExploitCheatGui"
gui.ResetOnSpawn = false

-- UI Setup (Centered Toggleable Menu)
local menuVisible = false

local frame = Instance.new("Frame", gui)
frame.Size = UDim2.new(0, 250, 0, 300)
frame.Position = UDim2.new(0.5, -125, 0.5, -150)
frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
frame.Visible = false

local toggleButton = Instance.new("TextButton", frame)
toggleButton.Size = UDim2.new(0, 240, 0, 50)
toggleButton.Position = UDim2.new(0, 5, 0, 10)
toggleButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
toggleButton.TextColor3 = Color3.new(1, 1, 1)
toggleButton.Text = "Toggle Menu"
toggleButton.Font = Enum.Font.SourceSansBold
toggleButton.TextSize = 18

local toggleFlightButton = Instance.new("TextButton", frame)
toggleFlightButton.Size = UDim2.new(0, 240, 0, 50)
toggleFlightButton.Position = UDim2.new(0, 5, 0, 70)
toggleFlightButton.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
toggleFlightButton.TextColor3 = Color3.new(1, 1, 1)
toggleFlightButton.Text = "Toggle Flight"
toggleFlightButton.Font = Enum.Font.SourceSansBold
toggleFlightButton.TextSize = 18

local spawn1x1Button = Instance.new("TextButton", frame)
spawn1x1Button.Size = UDim2.new(0, 240, 0, 50)
spawn1x1Button.Position = UDim2.new(0, 5, 0, 130)
spawn1x1Button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
spawn1x1Button.TextColor3 = Color3.new(1, 1, 1)
spawn1x1Button.Text = "Spawn 1x1x1x1"
spawn1x1Button.Font = Enum.Font.SourceSansBold
spawn1x1Button.TextSize = 18

local spawnRandomObjectButton = Instance.new("TextButton", frame)
spawnRandomObjectButton.Size = UDim2.new(0, 240, 0, 50)
spawnRandomObjectButton.Position = UDim2.new(0, 5, 0, 190)
spawnRandomObjectButton.BackgroundColor3 = Color3.fromRGB(255, 165, 0)
spawnRandomObjectButton.TextColor3 = Color3.new(1, 1, 1)
spawnRandomObjectButton.Text = "Spawn Random Object"
spawnRandomObjectButton.Font = Enum.Font.SourceSansBold
spawnRandomObjectButton.TextSize = 18

-- Cheat Variables
local flying = false
local bodyVelocity, bodyGyro
local flightSpeed = 50
local noclip = false

-- Toggle Menu Visibility
toggleButton.MouseButton1Click:Connect(function()
    menuVisible = not menuVisible
    frame.Visible = menuVisible
end)

-- 1x1x1x1 Model (Simulated)
local function create1x1x1x1()
    local model = Instance.new("Model")
    model.Name = "1x1x1x1"
    model.Parent = game.Workspace

    local bodyPart = Instance.new("Part")
    bodyPart.Size = Vector3.new(1, 1, 1)
    bodyPart.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(5, 0, 0)
    bodyPart.Anchored = true
    bodyPart.CanCollide = false
    bodyPart.BrickColor = BrickColor.new("Bright red")
    bodyPart.Parent = model
end

-- Flight Mode
local function enableFlight()
    if flying then
        if bodyVelocity then bodyVelocity:Destroy() end
        if bodyGyro then bodyGyro:Destroy() end

        bodyVelocity = Instance.new("BodyVelocity")
        bodyVelocity.MaxForce = Vector3.new(1e5, 1e5, 1e5)
        bodyVelocity.Velocity = Vector3.zero
        bodyVelocity.Parent = hrp

        bodyGyro = Instance.new("BodyGyro")
        bodyGyro.MaxTorque = Vector3.new(1e5, 1e5, 1e5)
        bodyGyro.CFrame = hrp.CFrame
        bodyGyro.Parent = hrp

        runService.Heartbeat:Connect(function()
            if flying then
                local direction = hrp.CFrame.LookVector * flightSpeed + Vector3.new(0, 2, 0)
                bodyVelocity.Velocity = direction
                bodyGyro.CFrame = hrp.CFrame
            end
        end)
    else
        if bodyVelocity then bodyVelocity:Destroy() end
        if bodyGyro then bodyGyro:Destroy() end
    end
end

-- Toggle Flight
toggleFlightButton.MouseButton1Click:Connect(function()
    flying = not flying
    enableFlight()
end)

-- Spawn Random Object
spawnRandomObjectButton.MouseButton1Click:Connect(function()
    local part = Instance.new("Part")
    part.Size = Vector3.new(10, 10, 10)
    part.Position = Vector3.new(math.random(-50, 50), 500, math.random(-50, 50))
    part.Anchored = true
    part.BrickColor = BrickColor.new("Bright yellow")
    part.Parent = game.Workspace
end)

-- Spawn 1x1x1x1 Model
spawn1x1Button.MouseButton1Click:Connect(function()
    create1x1x1x1()
end)

-- World Manipulation (Spawn large block)
local function editWorld()
    local part = Instance.new("Part")
    part.Size = Vector3.new(50, 5, 50)
    part.Position = game.Players.LocalPlayer.Character.HumanoidRootPart.Position + Vector3.new(0, 10, 0)
    part.Anchored = true
    part.CanCollide = true
    part.BrickColor = BrickColor.new("Bright green")
    part.Parent = game.Workspace

    -- Clone and stack blocks
    for i = 1, 10 do
        local newPart = part:Clone()
        newPart.Position = part.Position + Vector3.new(0, 10 * i, 0)
        newPart.Parent = game.Workspace
    end
end

-- Add more functionality if needed
-- Example: Run editWorld when needed

-- Final Setup: Toggle the UI and cheat buttons
frame.Visible = false
